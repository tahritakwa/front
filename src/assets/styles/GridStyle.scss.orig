.cta {
  position: relative;
  margin: auto;
  padding: 19px 22px;
  transition: all 0.2s ease;
}
.cta:before {
  content: "";
  position: absolute;
  top: 6px;
  left: 0;
  display: block;
  border-radius: 28px;
  background: rgba(67, 153, 174, 0.21176470588235294);
  width: 56px;
  height: 45px;
  -webkit-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
.cta span {
  position: relative;
  /* letter-spacing: 0.25em; */
  vertical-align: middle;
  background: none !important;
  font-size: 16px !important;
  margin-top: -0.8%;
  color: #999999 !important;
}
.cta svg {
  position: relative;
  top: 0;
  margin-left: 10px;
  fill: none;
  stroke-linecap: round;
  stroke-linejoin: round;
  stroke: #4399ae;
  stroke-width: 2;
  transform: translateX(-5px);
  transition: all 0.3s ease;
}
.cta:hover:before {
  width: 100%;
  background: #4c9aae;
}
.cta:hover span {
  color: white !important;
}
.cta:hover svg {
  color: white !important;
  transform: translateX(0);
  stroke: white;
}
.cta:active {
  transform: scale(0.96);
}


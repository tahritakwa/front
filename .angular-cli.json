{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "my-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "environments",
        "web.config"
      ],
      "prefix": "app",
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "styles": [
        "../node_modules/font-awesome/scss/font-awesome.scss",
        "../node_modules/@coreui/icons/scss/coreui-icons.scss",
        "../node_modules/simple-line-icons/scss/simple-line-icons.scss",
        "../node_modules/@coreui/coreui/scss/coreui.scss",
        "../node_modules/dragula/dist/dragula.css",
        "../node_modules/flag-icon-css/sass/flag-icon.scss",
        "../src/app/shared/components/reports/reporting-in-modal/kendo.common.scss",
        "../src/app/shared/components/reports/reporting-in-modal/kendo.blueopal.scss",
        "assets/styles/style.scss",
        "assets/styles/dashboard-styler.scss",
        "assets/styles/GridStyle.scss",
        "../node_modules/line-awesome/dist/line-awesome/scss/line-awesome.scss",
        "assets/styles/form.scss",
        "../node_modules/ng-snotify/styles/simple.scss",
        "../node_modules/primeng/resources/themes/omega/theme.css",
        "../node_modules/primeng/resources/primeng.min.css",
        "../node_modules/quill/dist/quill.core.css",
        "../node_modules/quill/dist/quill.snow.css",
        "../node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
        "../node_modules/fullcalendar/dist/fullcalendar.min.css",
        "../node_modules/intl-tel-input/build/css/intlTelInput.css",
        "../node_modules/@sweetalert2/theme-bootstrap-4/bootstrap-4.scss",
        "../node_modules/animate.css/animate.min.css",
        "../node_modules/@syncfusion/ej2-angular-diagrams/styles/material.css"
      ],
      "scripts": [
        "../node_modules/intl-tel-input/build/js/intlTelInput.min.js",
        "../node_modules/popper.js/dist/umd/popper.min.js",
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/quill/dist/quill.js",
        "../node_modules/moment/moment.js",
        "../node_modules/fullcalendar/dist/fullcalendar.js",
        "../node_modules/sweetalert2/dist/sweetalert2.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts",
        "qa": "environments/environment.qa.ts",
        "itg": "environments/environment.itg.ts",
        "rc": "environments/environment.rc.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {},
    "build": {
      "showCircularDependencies": false
    }
  }
}
